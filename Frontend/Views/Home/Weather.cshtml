@model List<WeatherForecast>

<h2>Weather Forecast</h2>

<table>
    <thead>
        <tr>
            <th>Date</th>
            <th>Temperature (C)</th>
            <th>Summary</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var forecast in Model)
        {
            <tr>
                <td>@forecast.Date.ToShortDateString()</td>
                <td>@forecast.TemperatureC</td>
                <td>@forecast.Summary</td>
            </tr>
        }
    </tbody>
</table>
